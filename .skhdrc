# open terminal
cmd - return : /Applications/Alacritty.app/Contents/MacOS/alacritty
# open browser
cmd + shift - return : open /Applications/Firefox.app
# open finder
cmd + shift + ctrl - return : open ~
# open dictionary
cmd + shift - d : open /System/Applications/Dictionary.app
# open calculator
cmd + shift - n : open /Applications/Numi.app
# open calculator
# cmd + shift - m : open  /System/Applications/Calendar.app/
cmd - up : /Applications/Alacritty.app/Contents/MacOS/alacritty msg config window.opacity=1.0
cmd - down : /Applications/Alacritty.app/Contents/MacOS/alacritty msg config window.opacity=0.5
